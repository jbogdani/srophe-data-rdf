        <rdfs:label xml:lang="en">Hwara, son of Theodore</rdfs:label>
        <lawd:hasName>
            <rdf:Description>
                <lawd:primaryForm xml:lang="en">Hwara, son of Theodore</lawd:primaryForm>
            </rdf:Description>
