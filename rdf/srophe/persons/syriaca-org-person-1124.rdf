            </rdf:Description>
        </lawd:hasName>
        <lawd:hasName>
            <rdf:Description>
                <lawd:variantForm xml:lang="fr-x-fiey">AḤḤA LE SOLITAIRE</lawd:variantForm>
                <lawd:hasAttestation rdf:resource="http://syriaca.org/bibl/650"/>
            </rdf:Description>
