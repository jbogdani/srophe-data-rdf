        <rdfs:label xml:lang="en">Dayr Ibn Jājī</rdfs:label>
        <lawd:hasName>
            <rdf:Description>
                <lawd:primaryForm xml:lang="en">Dayr Ibn Jājī</lawd:primaryForm>
            </rdf:Description>
        </lawd:hasName>
        <lawd:hasName>
            <rdf:Description>
                <lawd:variantForm xml:lang="ar">دير ابن جاجي</lawd:variantForm>
