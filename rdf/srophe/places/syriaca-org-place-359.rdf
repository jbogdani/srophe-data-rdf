    <rdf:Description rdf:about="http://syriaca.org/place/359">
        <rdf:type rdf:resource="http://lawd.info/ontology/Place"/>
        <rdfs:label xml:lang="en">Dayr Sarjīsiyya</rdfs:label>
        <rdfs:label xml:lang="syr">ܕܝܪܐ ܕܣܪܓܝܣܝܗ</rdfs:label>
