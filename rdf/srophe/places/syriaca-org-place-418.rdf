    <rdf:Description rdf:about="http://syriaca.org/place/418">
        <rdf:type rdf:resource="http://lawd.info/ontology/Place"/>
        <rdfs:label xml:lang="en">al-Ṣāliḥiyya</rdfs:label>
        <rdfs:label xml:lang="syr">ܨܐܠܚܝܗ̇</rdfs:label>
        <lawd:hasName>
            <rdf:Description>
                <lawd:primaryForm xml:lang="en">al-Ṣāliḥiyya</lawd:primaryForm>
            </rdf:Description>
        </lawd:hasName>
