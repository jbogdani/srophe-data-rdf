        <dcterms:description/>
        <dcterms:description>
            <rdf:Description>
                <dcterms:description xml:lang="syr-Syrj">
                         ܒܩܘܪܒܳܐ ܕܕܠܘܟ. ܘܚܳܐܰܪ ܠܡܰܪܓܐ ܕܥܰܝܢܳܐ ܘܥܰܡܝܪ ܗܘܳܐ  875  -  1000  ܘܢܰܦܶܩ  13  ܐܶܦܝܣܩܘܦ̈ܐ.  
                    </dcterms:description>
                <lawd:hasAttestation rdf:resource="http://syriaca.org/bibl/3"/>
            </rdf:Description>
        </dcterms:description>
        <dcterms:description>
            <rdf:Description>
                <dcterms:description xml:lang="ar">
                         بالثغر قرب دلوك يطلّ على مرج العين كان عامراً 875-1000 وخرّج ثلاثة عشر اسقفاً 
                    </dcterms:description>
                <lawd:hasAttestation rdf:resource="http://syriaca.org/bibl/2"/>
            </rdf:Description>
        </dcterms:description>
        <dcterms:description>
            <rdf:Description>
                <dcterms:description xml:lang="en">
                         in al-Thaghr, near Duluk, overlooking Marj al-ʿAyn, was inhabited between 875 and 1000. Thirteen bishops graduated from it. 
                    </dcterms:description>
                <lawd:hasAttestation rdf:resource="http://syriaca.org/bibl/4"/>
            </rdf:Description>
        </dcterms:description>
