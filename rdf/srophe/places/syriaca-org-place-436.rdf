        <dcterms:description>A village near  Ḥimṣ .</dcterms:description>
        <dcterms:description>
            <rdf:Description>
                <dcterms:description xml:lang="syr-Syrj">
                         ܡܶܢ ܩܘܪ̈ܝܐ ܕܰܚܡܶܨ ܠܡܰܕܢܚܳܐ ܡܝܰܬܒܳܐ ܒܣܘܪ̈ܝܝܐ. 
                    </dcterms:description>
                <lawd:hasAttestation rdf:resource="http://syriaca.org/bibl/3"/>
            </rdf:Description>
        </dcterms:description>
        <dcterms:description>
            <rdf:Description>
                <dcterms:description xml:lang="ar">
                         من قرى حمص الآهلة بالسريان شرقيها. 
                    </dcterms:description>
                <lawd:hasAttestation rdf:resource="http://syriaca.org/bibl/2"/>
            </rdf:Description>
        </dcterms:description>
        <dcterms:description>
            <rdf:Description>
                <dcterms:description xml:lang="en">
                         a village north of Ḥomṣ, heavily populated by Syrians. 
                    </dcterms:description>
                <lawd:hasAttestation rdf:resource="http://syriaca.org/bibl/4"/>
            </rdf:Description>
        </dcterms:description>
        <dcterms:description>Moosa's English translation of Barsoum's description says "north" when it should be "east."</dcterms:description>
