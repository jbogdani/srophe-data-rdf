        <dcterms:description>
            <rdf:Description>
                <dcterms:description xml:lang="syr-Syrj">
                         ܩܪܝܬܐ ܕܚܰܪܝܒܐ ܕܰܒܬܰܝܡܰܢ ܚܡܶܨ ܗܘܳܬ ܕܒܐܘܚܕܢܐ ܕܓܘܣܝܗ ܕܪܰܚܝܩܳܐ ܡܢܗ̇ ܡܰܪܕܶܐ ܫܒܰܥ ܫܳܥܝ̈ܢ. 
                    </dcterms:description>
                <lawd:hasAttestation rdf:resource="http://syriaca.org/bibl/3"/>
            </rdf:Description>
        </dcterms:description>
        <dcterms:description>
            <rdf:Description>
                <dcterms:description xml:lang="ar">
                         قرية مندثرة كانت جنوبي حمص من عمل جوسية مسافة سبع ساعات عنها 
                    </dcterms:description>
                <lawd:hasAttestation rdf:resource="http://syriaca.org/bibl/2"/>
            </rdf:Description>
        </dcterms:description>
        <dcterms:description>
            <rdf:Description>
                <dcterms:description xml:lang="en">
                         an extinct village in the province of Josya, seven hours journey south of Ḥomṣ. 
                    </dcterms:description>
                <lawd:hasAttestation rdf:resource="http://syriaca.org/bibl/4"/>
            </rdf:Description>
