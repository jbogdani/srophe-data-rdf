        <rdfs:label xml:lang="en">Dayro d-Beth Botin</rdfs:label>
        <rdfs:label xml:lang="syr">ܕܝܪܐ ܕܒܝܬ ܒܬܝ̈ܢ</rdfs:label>
        <lawd:hasName>
            <rdf:Description>
                <lawd:primaryForm xml:lang="en">Dayro d-Beth Botin</lawd:primaryForm>
            </rdf:Description>
        </lawd:hasName>
        <lawd:hasName>
            <rdf:Description>
                <lawd:primaryForm xml:lang="syr">ܕܝܪܐ ܕܒܝܬ ܒܬܝ̈ܢ</lawd:primaryForm>
