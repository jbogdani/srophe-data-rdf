        <dcterms:description/>
        <dcterms:description>
            <rdf:Description>
                <dcterms:description xml:lang="syr-Syrj">
                         ܡܶܢ ܩܘܪ̈ܝܳܐ ܕܠܰܚܩܐ ܗܘܳܬ ܒܩܘܽܪܒܐ ܕܩܰܨܪܐ ܕܒܰܪ ܗܘܒܝܪܗ ܕܒܐܽܘܚܕܳܢܐ ܕܟܽܘܦܰܗ ܠܐ ܡܶܬܚܰܙܝܐ ܕܘܟܬܗ̇. 
                    </dcterms:description>
                <lawd:hasAttestation rdf:resource="http://syriaca.org/bibl/3"/>
            </rdf:Description>
        </dcterms:description>
        <dcterms:description>
            <rdf:Description>
                <dcterms:description xml:lang="ar">
                         كانت من قرى السواد قرب قصر ابن هبيرة من عمل الكوفة  لا اثر لها 
                    </dcterms:description>
                <lawd:hasAttestation rdf:resource="http://syriaca.org/bibl/2"/>
            </rdf:Description>
        </dcterms:description>
        <dcterms:description>
            <rdf:Description>
                <dcterms:description xml:lang="en">
                         an ancient village in southern Iraq, near Qaṣr Ibn Hubayra, in the vicinity of al-Kūfa. 
                    </dcterms:description>
                <lawd:hasAttestation rdf:resource="http://syriaca.org/bibl/4"/>
