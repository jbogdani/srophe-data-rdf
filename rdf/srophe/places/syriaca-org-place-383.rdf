        <rdf:type rdf:resource="http://lawd.info/ontology/Place"/>
        <rdfs:label xml:lang="en">Mor Giwargis</rdfs:label>
        <rdfs:label xml:lang="syr">ܕܝܪܐ ܕܡܪܝ ܓܘܪܓܝܣ</rdfs:label>
