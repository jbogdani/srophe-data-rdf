        <dcterms:description/>
        <dcterms:description>
            <rdf:Description>
                <dcterms:description xml:lang="syr-Syrj">
                         ܒܐܘܚܕܳܢܐ ܕܰܩܠܝܣܘܪܐ. ܐܶܬܢܰܦܩܘ ܒܗ ܬܪܶܝܢ ܦܰܛܪܝܪ̈ܟܐ ܘܚܰܡܫܐ ܐܶܦܝܣ̈ܩܘܦܶܐ  686 - 1170 
                    </dcterms:description>
                <lawd:hasAttestation rdf:resource="http://syriaca.org/bibl/3"/>
            </rdf:Description>
        </dcterms:description>
        <dcterms:description>
            <rdf:Description>
                <dcterms:description xml:lang="ar">
                         في الشغر من أعمال قليسورا، فيه تخرج بطريركان وخمسة اساقفة 986 - 1170. 
                    </dcterms:description>
                <lawd:hasAttestation rdf:resource="http://syriaca.org/bibl/2"/>
            </rdf:Description>
        </dcterms:description>
        <dcterms:description>
            <rdf:Description>
                <dcterms:description xml:lang="en">
                         in Shaghr, in the province of Qallisura. Two patriarchs and five bishops graduated from it between 986 and 1170. 
                    </dcterms:description>
                <lawd:hasAttestation rdf:resource="http://syriaca.org/bibl/4"/>
            </rdf:Description>
        </dcterms:description>
