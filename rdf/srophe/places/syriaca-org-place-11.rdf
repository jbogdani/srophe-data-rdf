        <dcterms:description>
            <rdf:Description>
                <dcterms:description xml:lang="en">
                         One of the earliest Hellenistic city foundations in Syria, Apamea, situated on the Orontes ca. 90 km. south of Antioch, from the 2nd cent. onwards was an important center of Middle- and Neo-Platonist philosophy. 
                    </dcterms:description>
                <lawd:hasAttestation rdf:resource="http://syriaca.org/bibl/1"/>
            </rdf:Description>
        </dcterms:description>
        <dcterms:description>
            <rdf:Description>
                <dcterms:description xml:lang="en">
                         38. Apamea 
                    </dcterms:description>
                <lawd:hasAttestation rdf:resource="http://syriaca.org/bibl/1"/>
            </rdf:Description>
        </dcterms:description>
        <dcterms:description>
            <rdf:Description>
                <dcterms:description xml:lang="syr-Syrj">
                         ܡܕܝܢܬܳܐ ܗܘܳܬ ܪܰܒܬܐ ܕܰܠܓܰܪܒܰܝ ܡܰܕܢܰܚ ܚܡܳܬ ܘܚܰܪܝܒܳܐ ܗܳܫܐ ܘܡܶܬܐܶܡܰܪ ܠܗ̇ ܩܰܠܥܗ̈ ܐܠܡܨ̇ܝܩ. 
                    </dcterms:description>
                <lawd:hasAttestation rdf:resource="http://syriaca.org/bibl/3"/>
            </rdf:Description>
        </dcterms:description>
        <dcterms:description>
            <rdf:Description>
                <dcterms:description xml:lang="ar">
                         مدينة كانت كبيرة في الشمال الشرقي من حماة وهي الآن خربة يقال لها قلعة المضيق 
                    </dcterms:description>
                <lawd:hasAttestation rdf:resource="http://syriaca.org/bibl/2"/>
            </rdf:Description>
        </dcterms:description>
        <dcterms:description>
            <rdf:Description>
                <dcterms:description xml:lang="en">
                         once a large city northeast of Ḥama, it is now in ruins and is called Qalʿat al-Madīq. 
                    </dcterms:description>
                <lawd:hasAttestation rdf:resource="http://syriaca.org/bibl/4"/>
            </rdf:Description>
        </dcterms:description>
