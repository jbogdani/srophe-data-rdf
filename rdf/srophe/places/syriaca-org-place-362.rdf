        <dcterms:description/>
        <dcterms:description>
            <rdf:Description>
                <dcterms:description xml:lang="syr-Syrj">
                         ܒܩܘܪܒܳܐ ܕܐܘܪܗܳܝ. ܕܟܝܺܪ ܫܢܰܝ̈  512  -  565  ܐܰܚܪܒܗ ܥܳܫܘܡܐ ܥܰܒܕܐܠܠܗ ܒܰܪ ܒܰܟܬܰܪܝ  751.   
                    </dcterms:description>
                <lawd:hasAttestation rdf:resource="http://syriaca.org/bibl/3"/>
            </rdf:Description>
        </dcterms:description>
        <dcterms:description>
            <rdf:Description>
                <dcterms:description xml:lang="ar">
                         بقرب الرها ذكر سنة 512 و 565 و اخربه الغاشم عبد الله ابن البختري سنة 751 
                    </dcterms:description>
                <lawd:hasAttestation rdf:resource="http://syriaca.org/bibl/2"/>
            </rdf:Description>
        </dcterms:description>
        <dcterms:description>
            <rdf:Description>
                <dcterms:description xml:lang="en">
                         near Edessa, was mentioned in history in 512 and 565. It was destroyed in 751 by the tyrant ʿAbd Allah al-Bukhtūri. 
                    </dcterms:description>
                <lawd:hasAttestation rdf:resource="http://syriaca.org/bibl/4"/>
            </rdf:Description>
        </dcterms:description>
