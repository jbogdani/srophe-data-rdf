        <dcterms:description/>
        <dcterms:description>
            <rdf:Description>
                <dcterms:description xml:lang="syr-Syrj">
                         ܒܟܘܪܐ ܕܐܰܢܛܝܘܟܺܝܰܐ. ܕܟܺܝܪ ܒܰܫܢܰܬ 1112 ܘܡܶܢܗ ܢܦܰܩ ܚܰܕ ܦܰܛܪܝܰܪܟܐ ܘܐܰܪܒܥܳܐ ܐܶܦܝܣܩ̈ܘܦܶܐ. 
                    </dcterms:description>
                <lawd:hasAttestation rdf:resource="http://syriaca.org/bibl/3"/>
            </rdf:Description>
        </dcterms:description>
        <dcterms:description>
            <rdf:Description>
                <dcterms:description xml:lang="ar">
                         في كورة انطاكية ذكر سنة 1112 ومنه نشأ بطريرك واربعة اساقفة 
                    </dcterms:description>
                <lawd:hasAttestation rdf:resource="http://syriaca.org/bibl/2"/>
            </rdf:Description>
        </dcterms:description>
        <dcterms:description>
            <rdf:Description>
                <dcterms:description xml:lang="en">
                         in the province of Antioch, is first mentioned in history in 1112. From it came one Patriarch and four bishops. 
                    </dcterms:description>
                <lawd:hasAttestation rdf:resource="http://syriaca.org/bibl/4"/>
            </rdf:Description>
        </dcterms:description>
