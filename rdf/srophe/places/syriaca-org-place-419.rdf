        <dcterms:description/>
        <dcterms:description>
            <rdf:Description>
                <dcterms:description xml:lang="syr-Syrj">
                         ܆ܩܪܝܬܐ ܗܝ ܪܰܒܬܐ ܕܒܰܫܦܘܠܐ ܕܛܘܪܳܐ ܕܩܰܐܣܒܘܢ ܕܰܒܥܳܒܐ ܕܕܰܪܡܣܘܩ ܘܐܶܬܚܰܝܕܰܬ ܠܗ̇  
                    </dcterms:description>
                <lawd:hasAttestation rdf:resource="http://syriaca.org/bibl/3"/>
            </rdf:Description>
        </dcterms:description>
        <dcterms:description>
            <rdf:Description>
                <dcterms:description xml:lang="ar">
                         قرية كبيرة كانت في لحف جبل قاسيون من غوطة دمشق وقد اتصلت بها. 
                    </dcterms:description>
                <lawd:hasAttestation rdf:resource="http://syriaca.org/bibl/2"/>
            </rdf:Description>
        </dcterms:description>
        <dcterms:description>
            <rdf:Description>
                <dcterms:description xml:lang="en">
                         a large village which was situated in the Ghūta of Damascus, at the foot of Qasyun Mountain. It has now become part of Damascus. 
                    </dcterms:description>
                <lawd:hasAttestation rdf:resource="http://syriaca.org/bibl/4"/>
            </rdf:Description>
        </dcterms:description>
