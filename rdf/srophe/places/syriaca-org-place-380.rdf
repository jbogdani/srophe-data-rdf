        <dcterms:description/>
        <dcterms:description>
            <rdf:Description>
                <dcterms:description xml:lang="syr-Syrj">
                         ܒܐܽܘܚܕܳܢܐ ܕܓܰܪܡܰܐܢܺܝܩܝ (ܡܪܥܫ) ܐܶܬܢܰܦܰܩ ܒܗ ܐܰܪܒܥܐ ܐܶܦܝܣܩܘ̈ܦܐ ܡܶܢ ܫܢܰܬ  1100  -  1200   
                    </dcterms:description>
                <lawd:hasAttestation rdf:resource="http://syriaca.org/bibl/3"/>
            </rdf:Description>
        </dcterms:description>
        <dcterms:description>
            <rdf:Description>
                <dcterms:description xml:lang="ar">
                         في ولاية مرعش تخرج فيه اربعة اساقفة من سنة 1100 الى 1200. 
                    </dcterms:description>
                <lawd:hasAttestation rdf:resource="http://syriaca.org/bibl/2"/>
            </rdf:Description>
        </dcterms:description>
        <dcterms:description>
            <rdf:Description>
                <dcterms:description xml:lang="en">
                         in the province of Marʿash. Between 1100 and 1200 four bishops graduated from it. 
                    </dcterms:description>
                <lawd:hasAttestation rdf:resource="http://syriaca.org/bibl/4"/>
            </rdf:Description>
        </dcterms:description>
        <dcterms:temporal>1100/1200</dcterms:temporal>
